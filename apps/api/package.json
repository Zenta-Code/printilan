{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -p \"[{name}]\" -n \"TS,NODE\" -c \"yello.bold, cyan.bold\" \"pnpm run tsc:watch\" \"pnpm run start:hot\"",
    "build": "npm run tsc",
    "build:watch": "npm run tsc:watch",
    "tsc": "tsc",
    "tsc:watch": "tsc -w --preserveWatchOutput",
    "start": "node dist/index.js",
    "start:hot": "nodemon -e js,d.ts,js.map --watch dist/ \"dist/index.js\""
  },
  "dependencies": {
    "@prisma/client": "5.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express-list-endpoints": "^6.0.2",
    "@types/jsonwebtoken": "^9.0.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-list-endpoints": "^6.0.0",
    "express-route-grouping": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "logger": "^0.0.1",
    "mongoose": "^7.5.3",
    "morgan": "^1.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/morgan": "^1.9.5",
    "@types/node": "^15.14.9",
    "@types/supertest": "^2.0.13",
    "esbuild": "^0.14.54",
    "esbuild-register": "^3.5.0",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "^0.0.1",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "tsconfig": "^7.0.0",
    "typescript": "^4.9.5"
  }
}
